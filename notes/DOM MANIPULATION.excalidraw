    {
      "id": "rDaZGbEw6WCf9MZVrBcUN",
      "type": "text",
      "x": 144.08792154947895,
      "y": -699.2811369937262,
      "width": 752.1430045572921,
      "height": 175.64711217064036,
      "angle": 0,
      "strokeColor": "#1971c2",
      "backgroundColor": "transparent",
      "fillStyle": "solid",
